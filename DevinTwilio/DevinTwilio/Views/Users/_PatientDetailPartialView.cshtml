@model DevinTwilio.Models.UserViewModels
@{
    Layout = null;
}

<table class="table" id="UserData">
        <tr>
            <th>
                @Html.DisplayName("ID")
            </th>
            <td>
                @Html.DisplayFor(modelItem => Model.User_ID)
            </td>
            <th>

            </th>
            <td>@Html.ActionLink("Edit", "AddOrEdit", new { id = @Model.User_ID, actionDo= "Details" })</td>
        </tr>
        <tr>
            <th>
                @Html.DisplayName("Name")
            </th>
            <td>
                @Html.DisplayFor(modelItem => Model.User_Name)
            </td>
        </tr>
        <tr>
            <th>
                @Html.DisplayName("Phone")
            </th>
            <td>
                @Html.DisplayFor(modelItem => Model.MobileNumber)
            </td>
        </tr>
        <tr>
            <th>
                @Html.DisplayName("Enrollment Date")
