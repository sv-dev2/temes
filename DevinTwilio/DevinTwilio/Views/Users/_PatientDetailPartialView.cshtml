            </td>
        </tr>

    <tr>
        <th>
            @Html.DisplayName("Status")
        </th>
        <td>

            @Html.DisplayFor(modelItem => Model.UserStatus)
           
            @*@Html.ActionLink(@Model.UserStatus, "UpdateStatus", new { userId = @Model.User_ID })*@

           
        </td>
    </tr>


        <tr>
            <th>
                @Html.DisplayName("Notes")
